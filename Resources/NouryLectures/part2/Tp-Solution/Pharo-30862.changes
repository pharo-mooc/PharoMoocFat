idth	^ 8 @ 26! !!MaOsDeploymentWorld methodsFor: 'event handling' stamp: 'NouryBouraqadi 6/17/2014 09:18'!isCommandQ: evt	evt keyCharacter = 'q' ifFalse: [ ^false ].	evt commandKeyPressed ifFalse: [ ^false ].	{[evt altKeyPressed]. 	[evt controlKeyPressed ].	[evt optionKeyPressed].	[evt shiftPressed].	} do: [ :conditionBlock| conditionBlock ifTrue: [ ^false ] ].	^true! !!MaOsDeploymentWorld methodsFor: 'event handling' stamp: 'NouryBouraqadi 6/17/2014 09:18'!keyDown: evt	Smalltalk snapshot: false andQuit: true! !!MaOsDeploymentWorld methodsFor: 'geometry' stamp: 'NouryBouraqadi 6/12/2014 17:26'!shrinkWrap: morph	optimalExtent := morph optimalExtent. 	self forceOptimalExtent! !!MaApp class methodsFor: 'instance creation' stamp: 'NouryBouraqadi 1/5/2015 22:22'!forComponent: component	^self new		component: component;		yourself! !!MaApp class methodsFor: 'instance creation' stamp: 'NouryBouraqadi 1/5/2015 22:23'!forComponentClass: componentClass	^self forComponent: componentClass new! !!MaApp methodsFor: 'accessing' stamp: 'NouryBouraqadi 1/5/2015 18:49'!component	^ component! !!MaApp methodsFor: 'accessing' stamp: 'NouryBouraqadi 1/5/2015 18:50'!component: anObject	component := anObject! !!MaApp methodsFor: 'displaying' stamp: 'NouryBouraqadi 2/11/2014 10:01'!hide	self subclassResponsibility! !!MaApp methodsFor: 'accessing' stamp: 'NouryBouraqadi 1/5/2015 18:49'!name	^self component name! !!MaApp methodsFor: 'displaying' stamp: 'NouryBouraqadi 2/11/2014 10:01'!show	self subclassResponsibility! !!MaApp methodsFor: 'startup-shutdown' stamp: 'NouryBouraqadi 1/5/2015 21:47'!shutDown	"Pharo is quiting"	self component shutDown ! !!MaApp methodsFor: 'running' stamp: 'NouryBouraqadi 1/5/2015 22:00'!start	self show.	self component basicStart! !!MaApp methodsFor: 'startup-shutdown' stamp: 'NouryBouraqadi 1/5/2015 22:57'!startUp	"Pharo is starting up"	self component startUp.! !!MaApp methodsFor: 'running' stamp: 'NouryBouraqadi